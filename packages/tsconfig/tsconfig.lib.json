{
	"$schema": "https://json.schemastore.org/tsconfig",
	"display": "Library",
	"compilerOptions": {
		"target": "ESNext",
		"lib": ["ESNext", "DOM", "DOM.Iterable"],
		"module": "NodeNext",
		"moduleResolution": "NodeNext",
		"sourceMap": true,
		"declaration": true,
		"declarationMap": true,
		"isolatedDeclarations": true,
		"composite": true
	}
}
